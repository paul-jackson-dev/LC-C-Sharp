@using CodingEvents.Models
@model Tag

<h1>Add a New Tag</h1>

<form asp-controller="Tag" asp-action="ProcessAdd" method="post">
    <div class="form-group">
        <label asp-for="Name"></label>
        <input asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <input type="submit" value="Add a Tag" />
</form>